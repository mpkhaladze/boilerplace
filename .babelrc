{
  "presets": [
    [
      "@babel/preset-env",
      {
        "forceAllTransforms": true,
        "loose": true,
        "modules": false,
        "useBuiltIns": "entry"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "ramda",
    "lodash",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime"
  ]
}
